{
  "name": "contea",
  "version": "0.3.51",
  "description": "Chrome Extension",
  "license": "MIT",
  "author": "ConteMan",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:bg",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:bg": "vite build --config vite.config.bg.ts",
    "build:content": "vite build --config vite.config.content.ts",
    "watch": "npm run clear && run-p watch-build:web watch:prepare watch-build:bg",
    "watch:prepare": "esno scripts/prepare.ts",
    "watch-build:web": "vite build --watch",
    "watch-build:bg": "vite build --config vite.config.bg.ts --watch",
    "watch-build:content": "vite build --config vite.config.content.ts --watch",
    "clear": "rimraf extension/dist extension/manifest.json extension/version.json extension.*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@interactjs/core": "1.10.17",
    "@interactjs/utils": "1.10.17",
    "@vespaiach/axios-fetch-adapter": "^0.3.0",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.5",
    "dexie": "^3.2.2",
    "highlight.js": "^11.6.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.33.2",
    "octokit": "^2.0.7",
    "pinia": "2.0.21",
    "pinia-plugin-persistedstate": "2.1.1",
    "qs": "^6.11.0",
    "vue-router": "^4.1.5",
    "vue3-drr-grid-layout": "^1.9.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@iconify/json": "^2.1.103",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.15",
    "@types/qs": "^6.9.7",
    "@types/webextension-polyfill": "0.9.0",
    "@types/xmldom": "^0.1.31",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vueuse/components": "^9.3.0",
    "@vueuse/core": "^9.2.0",
    "@vueuse/head": "^0.7.9",
    "animate.css": "^4.1.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "kolorist": "^1.5.1",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.9",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.1.0",
    "vite-plugin-windicss": "^1.8.7",
    "vue": "^3.2.38",
    "vue-demi": "^0.13.11",
    "vuedraggable": "^4.1.0",
    "webextension-polyfill": "0.10.0",
    "windicss": "^3.5.6"
  }
}
