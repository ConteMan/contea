{
  "name": "contea",
  "version": "0.4.3",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "description": "Personal Chrome Extension",
  "author": {
    "name": "ConteMan",
    "email": "real.conteman@gmail.com",
    "url": "https://isconte.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/ConteMan/contea",
  "repository": {
    "type": "github",
    "url": "https://github.com/ConteMan/contea"
  },
  "keywords": [
    "extension"
  ],
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:bg build:content",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:bg": "vite build --config vite.config.bg.ts",
    "build:content": "vite build --config vite.config.content.ts",
    "watch": "npm run clear && run-p watch-build:web watch:prepare watch-build:bg watch-build:content",
    "watch:prepare": "esno scripts/prepare.ts",
    "watch-build:web": "vite build --watch",
    "watch-build:bg": "vite build --config vite.config.bg.ts --watch",
    "watch-build:content": "vite build --config vite.config.content.ts --watch",
    "clear": "rimraf extension/dist extension/manifest.json extension/version.json extension.*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@interactjs/core": "1.10.17",
    "@interactjs/utils": "1.10.17",
    "@realconteman/utils": "^0.0.5",
    "@vespaiach/axios-fetch-adapter": "^0.3.1",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "dexie": "^3.2.3",
    "highlight.js": "^11.8.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.3",
    "octokit": "^2.0.14",
    "pinia": "2.0.36",
    "pinia-plugin-persistedstate": "3.1.0",
    "qs": "^6.11.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@iconify/json": "^2.2.65",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.10",
    "@types/qs": "^6.9.7",
    "@types/webextension-polyfill": "0.10.0",
    "@types/xmldom": "^0.1.31",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "animate.css": "^4.1.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "kolorist": "^1.8.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "unocss": "^0.51.13",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^3.2.6",
    "vite-plugin-windicss": "^1.9.0",
    "vue": "^3.3.2",
    "vue-demi": "^0.14.1",
    "vuedraggable": "^4.1.0",
    "webextension-polyfill": "0.10.0",
    "windicss": "^3.5.6"
  }
}
