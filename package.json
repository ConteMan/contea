{
  "name": "contea",
  "displayName": "Contea",
  "version": "0.2.0",
  "description": "Personal Dashboard",
  "private": true,
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "dev:js": "npm run build:js -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:js",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:js": "vite build --config vite.config.content.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension_build.zip && jszip-cli add extension_build -o ./extension_build.zip",
    "pack:crx": "crx pack extension_build -o ./extension_build.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension_build --filename extension_build.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension_build --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension_build --target=firefox-desktop",
    "clear": "rimraf extension_build/dist extension_build/manifest.json extension_build.*",
    "lint": "eslint 'src/**/*.{json,ts,js,vue}'"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@antfu/eslint-config": "^0.16.1",
    "@ffflorian/jszip-cli": "^3.1.6",
    "@iconify/json": "^1.1.461",
    "@types/crypto-js": "^4.1.1",
    "@types/download": "8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.25",
    "@types/qs": "^6.9.7",
    "@types/webextension-polyfill": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.31",
    "@vueuse/core": "^7.6.2",
    "animate.css": "^4.1.1",
    "body-parser": "1.19.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "download": "8.0.0",
    "eslint": "^8.9.0",
    "esno": "^0.10.1",
    "express": "4.17.2",
    "fs-extra": "^10.0.0",
    "kolorist": "^1.5.1",
    "less": "^4.1.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "safe-compare": "1.1.4",
    "typescript": "^4.5.5",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-icons": "^0.11.4",
    "unplugin-vue-components": "^0.15.6",
    "vite": "^2.8.4",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.7.1",
    "vue": "^3.2.31",
    "vue-demi": "^0.11.4",
    "web-ext": "^6.7.0",
    "webext-bridge": "^5.0.3",
    "webextension-polyfill": "^0.8.0"
  },
  "dependencies": {
    "ant-design-vue": "^3.0.0-beta.10",
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "dexie": "^3.2.1",
    "fast-xml-parser": "^4.0.3",
    "highlight.js": "^11.4.0",
    "isomorphic-fetch": "^3.0.0",
    "kurimudb": "^5.6.1",
    "kurimudb-driver-dexie": "^5.2.3",
    "kurimudb-driver-localstorage": "^5.1.1",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.25.2",
    "octokit": "^1.7.1",
    "pinia": "^2.0.11",
    "pinia-plugin-persistedstate": "^1.2.2",
    "qs": "^6.10.3",
    "vue-router": "^4.0.12",
    "vuedraggable": "^4.1.0"
  }
}
