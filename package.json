{
  "name": "contea",
  "version": "0.3.84",
  "description": "Chrome Extension",
  "license": "MIT",
  "author": "ConteMan",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:bg build:content",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:bg": "vite build --config vite.config.bg.ts",
    "build:content": "vite build --config vite.config.content.ts",
    "watch": "npm run clear && run-p watch-build:web watch:prepare watch-build:bg watch-build:content",
    "watch:prepare": "esno scripts/prepare.ts",
    "watch-build:web": "vite build --watch",
    "watch-build:bg": "vite build --config vite.config.bg.ts --watch",
    "watch-build:content": "vite build --config vite.config.content.ts --watch",
    "clear": "rimraf extension/dist extension/manifest.json extension/version.json extension.*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@interactjs/core": "1.10.17",
    "@interactjs/utils": "1.10.17",
    "@vespaiach/axios-fetch-adapter": "^0.3.1",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "dexie": "^3.2.3",
    "highlight.js": "^11.7.0",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.3",
    "octokit": "^2.0.14",
    "pinia": "2.0.33",
    "pinia-plugin-persistedstate": "3.1.0",
    "qs": "^6.11.1",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@iconify/json": "^2.2.35",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.3",
    "@types/qs": "^6.9.7",
    "@types/webextension-polyfill": "0.10.0",
    "@types/xmldom": "^0.1.31",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/components": "^9.13.0",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.23",
    "animate.css": "^4.1.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "kolorist": "^1.7.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.0",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^3.2.5",
    "vite-plugin-windicss": "^1.8.10",
    "vue": "^3.2.47",
    "vue-demi": "^0.13.11",
    "vuedraggable": "^4.1.0",
    "webextension-polyfill": "0.10.0",
    "windicss": "^3.5.6"
  }
}
